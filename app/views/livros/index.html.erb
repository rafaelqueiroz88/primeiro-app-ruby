<div class="row">
  <div class="col">
      <p id="notice"><%= notice %></p>
  </div>
</div>
<div class="row">
  <div class="col">
      <h1>Livros</h1>
  </div>
</div>
<div class="row">
  <div class="col col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
    <table>
        <thead>
          <tr>
            <th>Titulo</th>
            <th colspan="3"></th>
          </tr>
        </thead>
      
        <tbody>
          <% @livros.each do |livro| %>
            <tr>
              <td><%= livro.titulo %></td>
              <td><%= link_to 'Show', livro %></td>
              <td><%= link_to 'Edit', edit_livro_path(livro) %></td>
              <td><%= link_to 'Destroy', livro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
  </div>
</div>
<br/>
<%= link_to 'New Livro', new_livro_path %>
